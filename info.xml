<?xml version="1.0"?>
<extension key="autologin_civi_tokens" type="module">
  <file>autologin_civi_tokens</file>
  <name>autologin_civi_tokens</name>
  <description>When drupal module autologin_url is enabled, add tokens to connect and redirect to webform/view or front page.
  Mainly used in mailings for members.
  </description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>olivier</author>
    <email>ol65@free.fr</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/olivierh65/autologin_civi_tokens.git</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-06-05</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.61</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/AutologinTokens</namespace>
    <format>23.02.1</format>
    <angularModule>crmAutologinTokens</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
